on fs
    start teecd
    start volisnotd

on property:vold.decrypt=trigger_post_fs_data
    trigger post-fs-data

service teecd /sbin/teecd
    disabled
    user root
    group root
    seclabel u:r:tee:s0

service volisnotd /sbin/volisnotd -m
      disabled
      user root
      group root
      seclabel u:r:tee:s0
